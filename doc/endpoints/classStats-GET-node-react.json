{
    "endpoint": "api/classStats",
    "method": "GET",
    "description": "Retrieve a list of a class stats",
    "request":{
        "body": {
            "idclass": "int"
        }
    },
    "response": {
        "200": {
            "status": "success",
            "description": "Class stats retrieved successfully",
            "body": [
                {
                    "id": "int",
                    "language_info": "string",
                    "quiz_answered": "string",
                    "data": "string (YYYY-MM-DD)"
                }
            ]
        }
    }
}
